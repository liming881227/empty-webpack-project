{
  "name": "empty-webpack-project",
  "version": "0.1.0",
  "description": "Empty Webpack Project",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build:dev && NODE_ENV=development webpack --inline --progress --colors --watch",
    "build:clean": "rm -rf public || echo 'No Public Folder Found'",
    "build:bundle": "NODE_ENV=production webpack --progress --colors",
    "build:img": "mkdir src/assets/img || echo 'Image Folder Already Exists.'",
    "build": "npm run build:clean && npm run build:bundle",
    "build:dev": "npm run build:clean && npm run build:img && mkdir public",
    "lint": "eslint src || echo 'Errors Found.'"
  },
  "author": "author <author@email.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.12.3",
    "browser-sync-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^1.0.1",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.7.0",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.8.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "sass-loader": "^3.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  }
}
