{
  "name": "empty-webpack-project",
  "version": "0.1.0",
  "description": "Empty Webpack Project",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build:dev && NODE_ENV=development webpack --inline --progress --colors --watch",
    "build:clean": "rm -rf public",
    "build:bundle": "NODE_ENV=production webpack --progress --colors",
    "build:php": "cp **/*.php public",
    "build": "npm run build:clean && npm run build:bundle && npm run build:php",
    "build:dev": "npm run build:clean && mkdir public && npm run build:php",
    "lint": "eslint src; exit 0"
  },
  "author": "author <author@email.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.12.3",
    "browser-sync-webpack-plugin": "^1.0.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^1.0.1",
    "eslint-plugin-react": "^5.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "strip-loader": "^0.1.2",
    "webpack": "^1.13.0"
  }
}
